<?xml version="1.0"?>
<robot name="robot_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Base -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
    </collision>
  </link>

  <!-- Link 1 -->
  <link name="link1">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.264"/>
      </geometry>
      <origin xyz="0 0 0.132" rpy="0 0 0"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.264"/>
      </geometry>
      <origin xyz="0 0 0.132" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="base_link_link1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" velocity="0.5" effort="1000.0"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>


  <!-- Link 2 -->
  <link name="link2">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.850"/>
      </geometry>
      <origin xyz="0 0 0.425" rpy="0 0 0"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.850"/>
      </geometry>
      <origin xyz="0 0 0.425" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="link1_link2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="1 0 0"/>
    <limit lower="-2.5" upper="2.5" velocity="0.5" effort="1000.0"/>
    <origin xyz="0.1 0 0.264" rpy="0 0 0"/>
  </joint>

  <!-- Link 3 -->
  <link name="link3">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.680"/>
      </geometry>
      <origin xyz="0 0 0.340" rpy="0 0 0"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.680"/>
      </geometry>
      <origin xyz="0 0 0.340" rpy="0 0 0"/>
    </collision>
  </link>
  
  <joint name="link2_link3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" velocity="0.5" effort="1000.0"/>
    <origin xyz="-0.1 0 0.850" rpy="0 0 0"/>
  </joint>

  <!-- Link 4 -->
  <link name="link4">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.130"/>
      </geometry>
      <origin xyz="0 0 0.065" rpy="0 0 0"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.130"/>
      </geometry>
      <origin xyz="0 0 0.065" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="link3_link4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" velocity="0.5" effort="1000.0"/>
    <origin xyz="0.1 0.0 0.680" rpy="0.0 0.0 0.0"/>
  </joint>

    <!-- Link 5 -->
  <link name="link5">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.170"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.170"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="link4_link5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" velocity="0.5" effort="1000.0"/>
    <origin xyz="0 0 0.180" rpy="1.570 0.0 0.0"/>
  </joint>

  <!-- Simple End Effector (Gripper base) -->
  <link name="gripper_base">
    <visual>
      <geometry>
        <box size="0.03 0.150 0.03"/>
      </geometry>
      <origin xyz="0 0 0.0125" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.03 0.0150 0.03"/>
      </geometry>
      <origin xyz="0 0 0.0125" rpy="0 0 0"/>
    </collision>
  </link>
  <joint name="link5_gripper_base" type="revolute">
    <parent link="link5"/>
    <child link="gripper_base"/>
    <origin xyz="0 0 0.085" rpy="0 0 1.57"/>
    <limit lower="-3.14" upper="3.14" velocity="0.5" effort="1000.0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Gripper Right -->
  <link name="gripper_right">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.10"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.01 0.01 0.10"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </collision>
  </link>
  <joint name="gripper_base_gripper_right" type="prismatic">
    <parent link="gripper_base"/>
    <child link="gripper_right"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="0.055" velocity="0.5" effort="1000.0"/>
    <origin xyz="0 -0.065 0.03" rpy="0 0 0"/>
  </joint>

  <!-- Gripper Left -->
  <link name="gripper_left">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.10"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.01 0.01 0.10"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </collision>
  </link>
  <joint name="gripper_base_gripper_left" type="prismatic">
    <parent link="gripper_base"/>
    <child link="gripper_left"/>
    <axis xyz="0 -1 0"/>
    <limit lower="0.0" upper="0.055" velocity="0.5" effort="1000.0"/>
    <origin xyz="0 0.065 0.03" rpy="0 0 0"/>
  </joint>


</robot>
